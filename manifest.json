{
    "manifest_version": 3,
    "name": "Toggle Search Engine",
    "version": "1.0",
    "description": "Toggle between popular search engines",
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
      }
    },
    "icons": {
      "128": "icon.png"
    },
    "permissions": ["activeTab", "scripting"]
}