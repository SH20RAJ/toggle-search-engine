{
    "manifest_version": 3,
    "name": "Toggle Search Engine",
    "version": "1.0",
    "description": "Toggle between Google and Bing search results",
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_icon": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
      }
    },
    "permissions": [
      "activeTab",
      "tabs"
    ]
  }
  